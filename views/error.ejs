<% layout('/layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger overflow-auto" role="alert">
            <h6 class="alert-heading"><%= err.message %></h6>
            <% if (process.env.NODE_ENV !== "production") { %>
            <p><%= err.stack %> </p>
            <% } %>
        </div>
    </div>
</div>